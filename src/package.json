{
  "name": "express-framework",
  "private": true,
  "version": "1.0.0",
  "description": "Docker based Express framework",
  "author": "Mike Farrow <contact@mikefarrow.co.uk>",
  "devDependencies": {
    "standard": "8.6.0",
    "forever": "0.15.3",
    "browser-sync": "2.18.7",
    "webpack": "2.2.1",
    "chokidar": "1.6.1",
    "webpack-dev-server": "2.3.0",
    "extract-text-webpack-plugin": "git://github.com/webpack-contrib/extract-text-webpack-plugin.git#v2.0.0-rc.2",
    "webpack-manifest-plugin": "1.1.0",
    "raw-loader": "0.5.1",
    "file-loader": "0.10.0",
    "url-loader": "0.5.7",
    "style-loader": "0.13.1",
    "css-loader": "0.26.1",
    "postcss-loader": "1.2.2",
    "postcss-import": "9.1.0",
    "postcss-assets": "4.1.0",
    "postcss-color-mix": "1.1.0",
    "postcss-color-function": "3.0.0",
    "postcss-responsive-images": "1.0.3",
    "postcss-media-minmax": "2.1.2",
    "postcss-cachebuster": "0.1.4",
    "lost": "8.0.0",
    "precss": "1.4.0",
    "autoprefixer": "6.7.2",
    "postcss-responsive-type": "0.5.0",
    "postcss-font-magician": "1.6.0",
    "normalize.css": "5.0.0",
    "blazy": "1.8.2"
  },
  "dependencies": {
    "app-module-path": "2.2.0",
    "express": "4.14.1",
    "body-parser": "1.17.2",
    "node-cache": "4.1.1",
    "request": "2.81.0",
    "cookies": "0.7.0",
    "debug": "2.6.0",
    "pug": "2.0.0-beta10",
    "lodash": "4.17.4",
    "async": "2.1.4",
    "morgan": "1.8.1",
    "prismic-dom": "1.2.8",
    "prismic-javascript": "1.1.4",
    "md5": "2.2.1",
    "slug": "0.9.1",
    "node-dir": "0.1.17",
    "snake-case": "2.1.0",
    "moment-timezone": "0.5.13"
  },
  "scripts": {
    "lint"    : "node_modules/.bin/standard",
    "dev"     : "node_modules/.bin/forever --minUptime 1000 --spinSleepTime 1000 node_modules/.bin/webpack-dev-server --colors --content-base public/ --host 0.0.0.0 --port 80 --hot --inline",
    "watch"   : "env $([ ! -f ../ENVIRONMENT_LOCAL ] || (cat ../ENVIRONMENT_LOCAL | xargs)) node_modules/.bin/forever --minUptime 1000 --spinSleepTime 1000 index.js",
    "test"    : "npm run lint",
    "webpack" : "node_modules/.bin/webpack",
    "tunnel"  : "node_modules/localtunnel/bin/client -p 80 -s $TUNNEL_PREFIX"
  }
}
